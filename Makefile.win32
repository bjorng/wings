MFLAGS = -f Makefile.win32

all:
	cd src 
	$(MAKE) $(MFLAGS) 
	cd ..\e3d 
	$(MAKE) $(MFLAGS)
	cd ..\plugins_src 
	$(MAKE) $(MFLAGS)
	cd ..\icons
	$(MAKE) $(MFLAGS)
	cd ..
clean:
	cd src 
	$(MAKE) $(MFLAGS) clean 
	cd ..\e3d 
	$(MAKE) $(MFLAGS) clean
	cd ..\plugins_src 
	$(MAKE) $(MFLAGS) clean
	cd ..\icons
	$(MAKE) $(MFLAGS) clean
	cd ..
